{
    "name": "sierratecnologia/audit",
    "description": "Audit",
    "type": "library",
    "keywords": [
        "sierratecnologia",
        "audit"
    ],
    "license": "MIT",
    "homepage": "https://sierratecnologia.com/packages/audit/",
    "support": {
        "email": "help@sierratecnologia.com",
        "issues": "https://github.com/sierratecnologia/audit/issues",
        "source": "https://github.com/sierratecnologia/audit",
        "docs": "https://github.com/sierratecnologia/audit/blob/master/README.md"
    },
    "authors": [
        {
            "name": "SierraTecnologia",
            "homepage": "https://sierratecnologia.com.br",
            "email": "help@sierratecnologia.com.br"
        },
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": "^7.4",
        "sierratecnologia/muleta": "dev-master",
        "sierratecnologia/pedreiro": "dev-master",
        "kassner/log-parser": "^2.0",
        "rap2hpoutre/laravel-log-viewer": "^1.5",
        "laravel/telescope": "^3.0 || ^4.0",
        "spatie/laravel-activitylog": "^3.10 || ^4.0"
    },
    "suggest": {
        "owen-it/laravel-auditing": "^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "orchestra/testbench": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.2",
        "vimeo/psalm": "^3.11"
    },
    "autoload": {
        "psr-4": {
            "Audit\\": "src/"
        }
    },
    "bin": ["bin/audit"],
     "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "check-style": "phpcs -p --standard=PSR12 src --extensions=php",
        "fix-style": "phpcbf -p --standard=PSR12 src --extensions=php"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Audit\\AuditProvider"
            ]
        }
    }
}
