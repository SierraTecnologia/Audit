{
    "name": "sierratecnologia/audit",
    "description": "Audit",
    "license": "MIT",
    "authors": [
        {
            "name": "Ricardo Rebello Sierra",
            "email": "contato@ricardosierra.com.br"
        }
    ],
    "require": {
        "php": ">=7.0",
        "kassner/log-parser": "*",
        "rap2hpoutre/laravel-log-viewer": "*",
        "laravel/telescope": "^2.0",
        "spatie/laravel-activitylog": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "Audit\\": "src/"
        }
    },
    "bin": ["bin/audit"],
    "scripts": {
        "post-install-cmd": [],
        "post-update-cmd": [],
        "post-create-project-cmd": []
    },
    "repositories": [
        {
            "type": "path",
            "url": "../**",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Audit\\AuditProvider"
            ]
        }
    }
}
